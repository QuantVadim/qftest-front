escription?.length\" class=\"card-description\">\r\n        <pre>{{ data.description}}</pre>\r\n        </div>\r\n      </div>\r\n      <div>\r\n        <Knob v-model=\"score\" :min=\"0\" :max=\"100\" :size=\"60\" :valueTemplate=\"score+'%'\" readonly />\r\n        <div class=\"result-card-score\">{{ data.score+\"/\"+data.max_score }}</div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Knob from 'primevue/knob';\r\n\r\nexport default {\r\n  components:{\r\n    Knob\r\n  },\r\n  props: ['data'],\r\n  data(){\r\n    return{\r\n      score: Math.floor((+this.data.score/+this.data.max_score)*100),\r\n    }\r\n  },\r\n  computed: {\r\n    date_created(){\r\n      if(this?.data?.date_created){\r\n      let arr = this.data.date_created.split(/[- :]/);\r\n      return arr[2]+\".\"+arr[1]+' в '+arr[3]+':'+arr[4];\r\n      }else return \"\";\r\n    }\r\n  },\r\n  methods:{\r\n    goTest(){\r\n      this.$router.push(`/test/${this.data.ref_test_id}`);\r\n    },\r\n    goResult(){\r\n      this.$router.push(`/result/${this.data.res_id}`);\r\n    }\r\n  }\r\n\r\n}\r\n</script>\r\n<style scoped>\r\n.result-card-score{\r\n  text-align: center;\r\n  color: gray;\r\n}\r\n.card-result-body{\r\n  display: grid;\r\n  grid-template-columns: 1fr auto;\r\n}\r\n.card-test-title span:hover{\r\n  cursor: pointer;\r\n  text-decoration: underline;\r\n  text-underline-offset: 2px;\r\n}\r\n\r\n.it-icon{\r\n  cursor: pointer;\r\n}\r\n\r\n.card_test_top_header{\r\n  display: grid;\r\n  grid-template-columns: 1fr auto;\r\n}\r\npre{\r\n  margin: 0px\r\n}\r\nh3{\r\n  margin: 0px;\r\n}\r\n.card-autor-name{\r\n  color: gray;\r\n}\r\n.card-description{\r\n  max-height: 300px;\r\n  overflow-y: auto;\r\n}\r\n</style>"],"sourceRoot":""}]}