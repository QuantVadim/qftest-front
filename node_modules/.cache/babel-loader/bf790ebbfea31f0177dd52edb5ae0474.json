AC;AAEH,GALX,E,IAAA,E,CAAA,E,yBAAA,CAMI,CAVN,CAmCI,EAvBJ,YAAA,CAUM,KAVN,EAAA,UAAA,EAUM,CATJ,YAAA,CAOM,KAPN,EAOM,IAPN,EAOM,CANJ,YAAA,CAKE,kBALF,EAKE;AAJC,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,UAAA,MAAA;AAAA,aAAE,KAAA,CAAA,MAAA,GAAM,CAAI,KAAA,CAAA,MAAZ;AAAA,KAAA,CAIN;AAHA,IAAA,IAAI,EAAC,aAGL;AAFC,IAAA,KAAK,EAAA,CAAA;AAAA,qBAAmB,KAAA,CAAA;AAAnB,KAAA,EACA,iBADA;AAEN,GALF,E,IAAA,E,CAAA,E,SAAA,CAMI,CAPN,CASI,EADJ,YAAA,CAAgD,KAAhD,EAAA,UAAA,EAAgD,gBAAA,CAAjB,KAAA,CAAA,EAAA,CAAG,KAAc,CAAhD,EAAuC,CAAvC,CACI,CAVN,CAuBI,EAXO,KAAA,CAAA,M,kBAAX,YAAA,CAUM,KAVN,EAAA,UAAA,EAUM,CATJ,YAAA,CAQM,KARN,EAAA,UAAA,EAQM,CAPJ,WAOI,EANJ,YAAA,CAKE,0BALF,EAKE;gBAJS,KAAA,CAAA,EAAA,CAAG,KAIZ;;aAJS,KAAA,CAAA,EAAA,CAAG,KAAH,GAAQ,M;MAIjB;AAHC,IAAA,GAAG,EAAE,CAGN;AAFC,uBAAiB,IAElB;AADC,IAAA,GAAG,EAAE;AACN,GALF,E,IAAA,E,CAAA,E,cAAA,CAMI,CARN,CASI,CAVN,C,iCAWI,CAzDN,C,mBA0DA,YAAA,CAAuB,KAAvB,EAAuB,WAAvB,EAAY,OAAZ,C,CACI,CA5DN,C","sourcesContent":["<template>\r\n  <div>\r\n    <div v-if=\"dt != undefined\">\r\n      <div class=\"choice-options\">\r\n        <it-switch class=\"choice-option\" v-model=\"dt.isMultiple\" label=\"Множественный выбор\" />\r\n      </div>\r\n      <it-textarea\r\n        v-model=\"dt.text\"\r\n        placeholder=\"Вопрос\"\r\n        :resize-on-write=\"true\"\r\n        :rows=\"1\"\r\n      ></it-textarea>\r\n      <div class=\"choices-body\">\r\n        <div v-for=\"(item, index) in dt.choices\" :key=\"index\" class=\"choice-line\">\r\n          <it-checkbox v-if=\"dt.isMultiple\" v-model=\"dt.choices[index].selected\"> </it-checkbox>\r\n          <it-radio v-else v-model=\"dt.answer\" :value=\"dt.choices[index].id\"/>\r\n          <span @click=\"setEditingMode(dt.choices[index].id)\">{{\r\n            dt.choices[index].label\r\n          }}</span>\r\n          <div>\r\n            <it-icon @click=\"deleteChoice(index)\" class=\"btn-choice-delete\" name=\"highlight_off\" color=\"#838996\" />\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div class=\"choice-input-line\">\r\n        <div>\r\n          <it-button @click=\"setEditingMode(false)\" size=\"small\" v-if=\"curIdEditing != undefined\">X</it-button>\r\n        </div>\r\n        <it-input\r\n          v-model=\"inputChoice\"\r\n          @keyup.enter=\"EnterChice\"\r\n          placeholder=\"Добавить пункт\"\r\n        >\r\n        </it-input>\r\n      </div>\r\n\r\n      <div class=\"card-more-footer\">\r\n        <div>\r\n          <it-icon\r\n            @click=\"isMore = !isMore\"\r\n            name=\"expand_more\"\r\n            :class=\"{ 'more-rotate': isMore }\"\r\n            class=\"btn-expand-more\"\r\n          />\r\n        </div>\r\n        <div class=\"mini-gray-text\">{{ dt.score }}</div>\r\n      </div>\r\n\r\n      <div v-if=\"isMore\" class=\"more-props-wrapper\">\r\n        <div class=\"prop-line\">\r\n          <div>Кол-во баллов</div>\r\n          <it-number-input\r\n            v-model=\"dt.score\"\r\n            :min=\"1\"\r\n            :resize-on-write=\"true\"\r\n            :max=\"100\"\r\n          />\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <div v-else>Пусто</div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n\r\nexport default {\r\n  props: [\"data\"],\r\n  data() {\r\n    return {\r\n      dt: this.data,\r\n\r\n      curIdEditing: undefined,\r\n      inputChoice: \"\",\r\n      isMore: false,\r\n    };\r\n  },\r\n  mounted() {\r\n    this.dt.text = this.data?.text ? this.data.text : '';\r\n    this.dt.score = this.data?.score ? this.data.score : 1;\r\n    this.dt.choices = this.data?.choices ? this.data.choices : [];\r\n    this.dt.isMultiple = this.data?.isMultiple ? this.data.isMultiple : false;\r\n    this.dt.answer = this.data?.answer ? this.data.answer : undefined;\r\n\r\n    console.log(this.dt);\r\n  },\r\n  methods: {\r\n    deleteChoice(index){\r\n      this.dt.choices.splice(index, 1);\r\n    },\r\n    setEditingMode(id) {\r\n      let obj = {};\r\n      if (id != false) {\r\n        this.dt.choices.forEach((element) => {\r\n          if (element.id == id) obj = element;\r\n        });\r\n        if (obj != undefined) {\r\n          this.curIdEditing = id;\r\n          this.inputChoice = obj.label;\r\n        }\r\n      }else{\r\n        this.curIdEditing = undefined;\r\n        this.inputChoice = '';\r\n      }\r\n    },\r\n    EnterChice() {\r\n      if(this.inputChoice.trim().length == 0) return false;\r\n      let obj = undefined;\r\n      if (this.curIdEditing == undefined) {\r\n        obj = {\r\n          id: Math.random(),\r\n          label: this.inputChoice.trim(),\r\n          selected: false,\r\n        };\r\n        this.dt.choices.splice(this.dt.choices.length, 0, obj);\r\n      } else {\r\n        let indexInsert = this.dt.choices.length;\r\n        for (let index = 0; index < this.dt.choices.length; index++) {\r\n          const element = this.dt.choices[index];\r\n          if (element.id == this.curIdEditing) {\r\n            indexInsert = index;\r\n            obj = this.dt.choices[index];\r\n          }\r\n        }\r\n        if(obj){\r\n           obj.label = this.inputChoice.trim();\r\n           this.dt.choices.splice(indexInsert, 1, obj);\r\n        }else{\r\n          obj = {\r\n          id: Math.random(),\r\n          label: this.inputChoice.trim(),\r\n          selected: false,\r\n          }\r\n          this.dt.choices.splice(indexInsert, 0, obj);\r\n        }\r\n      }\r\n      this.inputChoice = \"\";\r\n      this.curIdEditing = undefined;\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.choice-options {\r\n  padding-bottom: 6px !important;\r\n  opacity: 0.23;\r\n  transition: opacity 0.2s;\r\n}\r\n.choice-options:hover{\r\n  opacity: 1;\r\n}\r\n.choice-input-line {\r\n  display: grid;\r\n  grid-template-columns: auto 1fr;\r\n}\r\n.btn-choice-delete{\r\n  margin-left: 8px;\r\n}\r\n.choice-input-line .it-btn {\r\n  height: 32px !important;\r\n  width: 32px !important;\r\n}\r\n.choice-line {\r\n  display: flex;\r\n  margin-top: 6px;\r\n  margin-bottom: 6px;\r\n}\r\n.choice-line>*{\r\n  cursor: pointer;\r\n}\r\n.prop-line{\r\n  margin-top: 10px !important;\r\n}\r\n\r\n</style>"],"sourceRoot":""}]}