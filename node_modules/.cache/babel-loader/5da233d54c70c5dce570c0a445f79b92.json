,YAJO,wBAIM,IAJN,EAIW;AAChB,aAAO,KAAK,MAAL,CAAY,IAAZ,IAAoB,IAAG,IAAK,IAAI,CAAC,WAAL,EAAR,IAA8B,IAAG,IAAK,GAA1D,GAAgE,KAAvE;AACD,KANM;AAOP,IAAA,cAPO,0BAOQ,KAPR,EAOc;AACnB,UAAI,IAAG,GAAI,KAAK,EAAL,CAAQ,IAAR,CAAa,KAAb,CAAX;;AACA,UAAG,KAAK,MAAL,CAAY,IAAZ,CAAH,EAAqB;AACnB,QAAA,IAAG,GAAI,IAAI,CAAC,WAAL,MAAsB,IAAtB,GAA6B,IAAI,CAAC,WAAL,EAA7B,GAAkD,IAAI,CAAC,WAAL,EAAzD;AACA,YAAI,CAAA,GAAI,KAAK,EAAL,CAAQ,IAAhB;AACA,aAAK,EAAL,CAAQ,IAAR,GAAe,CAAC,CAAC,MAAF,CAAS,CAAT,EAAY,KAAZ,IAAmB,IAAnB,GAAwB,CAAC,CAAC,MAAF,CAAS,KAAK,GAAC,CAAf,CAAvC;AACF;AACF;AAdO;AAfI,CAAf","sourcesContent":["<template>\r\n  <div>\r\n    <div v-if=\"dt != undefined\">\r\n      <div></div>\r\n      <it-textarea\r\n        v-model=\"dt.text\"\r\n        placeholder=\"Контекст\"\r\n        :resize-on-write=\"true\"\r\n        :rows=\"1\"\r\n      ></it-textarea>\r\n      <div class=\"line-word\">\r\n        <span v-for=\"(item, index) in dt.word\" \r\n          :key=\"index\"\r\n          :class=\"{'letter-glas':isGlas(item), \r\n            'letter-percussive':isPercussive(item),\r\n            'letter-space': item == ' '}\" \r\n          @click=\"switchEmphasis(index)\"\r\n        >\r\n        {{item.toLowerCase()}}\r\n        </span>\r\n      </div>\r\n      <it-input v-model=\"dt.word\" placeholder=\"Слово\"></it-input>\r\n      <div class=\"card-more-footer\">\r\n        <div>\r\n          <it-icon\r\n            @click=\"isMore = !isMore\"\r\n            name=\"expand_more\"\r\n            :class=\"{ 'more-rotate': isMore }\"\r\n            class=\"btn-expand-more\"\r\n          />\r\n        </div>\r\n        <div class=\"mini-gray-text\">{{ dt.score }}</div>\r\n      </div>\r\n      <div v-if=\"isMore\" class=\"more-props-wrapper\">\r\n        <div class=\"prop-line\">\r\n          <div>Кол-во баллов</div>\r\n          <it-number-input\r\n            v-model=\"dt.score\"\r\n            :min=\"1\"\r\n            :resize-on-write=\"true\"\r\n            :max=\"100\"\r\n          />\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <div v-else>Пусто</div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n//  props:{ data: {type: Object, default: {id: Math.random(), type=\"simple\" , text: \"\", answer: \"\"} },\r\n\r\nexport default {\r\n  props: [\"data\"],\r\n  data() {\r\n    return {\r\n      dt: this.data,\r\n      isMore: false,\r\n      glas: ['а', 'о', 'э', 'е', 'и', 'ы', 'у', 'ё', 'ю', 'я'],\r\n    };\r\n  },\r\n  mounted() {\r\n    this.dt.text = this.data?.text ? this.data.text : '';\r\n     this.dt.word = this.data?.word ? this.data.word : '';\r\n    this.dt.score = this.data.score ? this.data.score : 1;\r\n\r\n  },\r\n  methods: {\r\n    isGlas(char){\r\n      return this.glas.indexOf(char.toLowerCase()) >= 0 ? true : false;\r\n    },\r\n    isPercussive(char){\r\n      return this.isGlas(char) ? char == char.toUpperCase() || char == 'ё' : false;\r\n    },\r\n    switchEmphasis(index){\r\n      let char = this.dt.word[index];\r\n      if(this.isGlas(char)){\r\n        char = char.toUpperCase() == char ? char.toLowerCase() : char.toUpperCase();\r\n        let w = this.dt.word;\r\n        this.dt.word = w.substr(0, index)+char+w.substr(index+1); \r\n      }\r\n    } \r\n\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.letter-space{\r\n  padding-right: 10px;\r\n}\r\n.letter-glas{\r\n\r\n  border: 1px solid #63b7fc5b;\r\n  padding-left: 4px;\r\n  padding-right: 4px;\r\n  border-radius: 3px;\r\n  cursor: pointer;\r\n  box-shadow: 0px 0px 0px 0px #64b3f300;\r\n}\r\n.letter-glas:hover{\r\n  box-shadow: 0px 0px 2px 1px #64b2f3;\r\n}\r\n.letter-percussive{\r\n  background-color: #5bb8ff;\r\n  border: 1px solid #64b2f3;\r\n  color: white;\r\n}\r\n\r\n.line-word{\r\n  min-height: 40px;\r\n  overflow-x: auto;\r\n  text-align: center;\r\n  line-height: 40px;\r\n  font-size: 28px;\r\n  margin-top: 6px;\r\n  margin-bottom: 10px;\r\n}\r\n\r\n</style>"],"sourceRoot":""}]}