         <div class=\"h-vk-enter\" v-else>\r\n            <it-button outlined text class='h-enter-vk' @click=\"enterVK\">Вход</it-button>\r\n          </div>\r\n      </div>\r\n    </div>\r\n\r\n    <it-drawer v-model=\"menuIsActive\" width=\"250px\" placement=\"left\">\r\n      <div>\r\n        <h3>Меню</h3>\r\n        <it-button-group vertical>\r\n          <it-button block @click=\"goRoute('/')\">Home</it-button>\r\n          <it-button block @click=\"goRoute('/test/new/editor')\">Создать тест</it-button>\r\n        </it-button-group>\r\n      </div>\r\n    </it-drawer>\r\n  </header>\r\n\r\n  <router-view :key=\"$route.fullPath\" />\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      menuIsActive: false,\r\n    };\r\n  },\r\n  computed: {\r\n    mainProfile() {\r\n      return \"/user/\" + this.$store.state.ME.data.usr_id;\r\n    },\r\n    MYID(){\r\n        return this.$store.getters.MYID;\r\n    }\r\n  },\r\n  methods: {\r\n    goRoute(route) {\r\n      this.$router.push(route);\r\n    },\r\n    enterVK(){\r\n        document.location = 'https://oauth.vk.com/authorize?client_id=7870026&display=page&redirect_uri=http://localhost:8080/auth/vk&scope=offline&response_type=token&v=5.52';\r\n    }\r\n  },\r\n};\r\n</script>\r\n\r\n\r\n<style scope>\r\n.h-enter-vk{\r\ndisplay: block;\r\ncolor: white !important;\r\nheight: 41px;\r\n}\r\n.header_user_info{\r\n    color: white;\r\n    padding: 4px;\r\n}\r\nheader .it-drawer-body h3 {\r\n  padding: 0px 6px;\r\n  margin: 0px;\r\n  margin-bottom: 10px;\r\n}\r\nheader .it-drawer-body > div {\r\n  margin: 16px;\r\n}\r\nheader i {\r\n  padding: 5px;\r\n  padding-left: 10px;\r\n  font-size: 32px !important;\r\n  width: 32px;\r\n  height: 32px;\r\n  background-size: cover;\r\n}\r\nheader {\r\n  background-color: rgb(72, 101, 156);\r\n  height: 42px;\r\n}\r\n.header-content{\r\n  display: grid;\r\n  grid-template-columns: auto 1fr auto;\r\n}\r\n</style>"],"sourceRoot":""}]}