еобходимо авторизироваться'\"/>\n    </block>\n  </div>\n</template>\n\n<script>\nimport TestCards from \"../../components/TestCards\";\n\nexport default {\n  components: {\n    TestCards,\n  },\n  computed:{\n    MYID(){return this.$store.getters.MYID;}\n  },\n  data() {\n    return {\n      test: undefined,\n    };\n  },\n  methods:{\n    SendTest(){\n      let obj = {\n        q: 'test_send',\n        me: this.$store.state.ME.data,\n        test: this.test,\n      }\n      this.axios.post(window.c.API, obj).then(itm => {\n        console.log(itm.data);\n        if(itm.data?.data){\n          let test_id = itm.data?.data;\n          this.$router.replace({path: `/test/${test_id}/editor`})\n        }\n      });\n    }\n  },\n  created(){\n    //(this.$route.params.id == 'new')\n      let obj = {\n        q: 'get_test_basic',\n        me: this.$store.state.ME.data,\n        test_id: this.$route.params.id,\n      }\n      this.axios.post(window.c.API, obj).then(itm => {\n        console.log(itm.data);\n        if(itm.data?.data){\n          this.test = itm.data.data;\n        }\n      });\n    \n  },\n};\n</script>\n\n<style>\ntextarea.it-textarea {\n  overflow: hidden !important;\n}\n</style>\n"]}]}